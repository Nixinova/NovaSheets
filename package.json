{
    "name": "novasheets",
    "version": "1.0.0-rc6",
    "description": "A simple, powerful, and versatile CSS preprocessor",
    "keywords": [
        "novasheets",
        "nvss",
        "css",
        "css preprocessor",
        "preprocessor",
        "stylesheets",
        "parser"
    ],
    "files": [
        "src/*.js",
        "src/*.d.ts",
        "!src/browser.*"
    ],
    "main": "src/novasheets.js",
    "bin": {
        "novasheets": "src/cli.js"
    },
    "scripts": {
        "prepublish": "tsc",
        "build": "tsc && webpack",
        "test": "tsc && qunit && jake test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NovaSheets/NovaSheets.git"
    },
    "bugs": {
        "url": "https://github.com/NovaSheets/NovaSheets/issues"
    },
    "author": "Nixinova (https://nixinova.com)",
    "homepage": "https://novasheets.js.org",
    "license": "ISC",
    "dependencies": {
        "balanced-match": "^2.0.0",
        "glob": "^7.1.7",
        "math-and-unit-parser": "^1.3.6",
        "yargs-parser": "^20.2.7"
    },
    "devDependencies": {
        "@types/node": "ts4.3",
        "jake": "^10.8.2",
        "qunit": "^2.16.0",
        "typescript": "~4.3.2",
        "webpack": "^5.38.1",
        "webpack-cli": "^4.7.2"
    }
}
