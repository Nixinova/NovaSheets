{
    "name": "novasheets",
    "version": "1.0.0-pre3",
    "description": "A simple, powerful, and versatile CSS preprocessor",
    "keywords": [
        "novasheets",
        "nss",
        "css",
        "css preprocessor",
        "preprocessor",
        "parser"
    ],
    "files": [
        "src/*.js"
    ],
    "main": "src/index.js",
    "bin": {
        "novasheets": "src/index.js"
    },
    "scripts": {
        "prepublish": "tsc",
        "postinstall": "node .",
        "build": "tsc && node build/browser",
        "test": "qunit && jake test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NovaSheets/NovaSheets.git"
    },
    "author": "Nixinova (https://nixinova.com)",
    "bugs": {
        "url": "https://github.com/NovaSheets/NovaSheets/issues"
    },
    "homepage": "https://novasheets.nixinova.com",
    "license": "ISC",
    "dependencies": {
        "glob": "^7.1.6"
    },
    "devDependencies": {
        "@types/node": "^14.14.31",
        "jake": "^10.8.2",
        "qunit": "^2.14.0",
        "typescript": "^4.2.2"
    }
}
