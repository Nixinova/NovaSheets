{
    "name": "novasheets",
    "version": "1.0.0-rc5",
    "description": "A simple, powerful, and versatile CSS preprocessor",
    "keywords": [
        "novasheets",
        "nvss",
        "css",
        "css preprocessor",
        "preprocessor",
        "stylesheets",
        "parser"
    ],
    "files": [
        "src/*.js",
        "src/*.d.ts",
        "!src/browser.*"
    ],
    "main": "src/novasheets.js",
    "bin": {
        "novasheets": "src/cli.js"
    },
    "scripts": {
        "prepublish": "tsc",
        "build": "tsc && node build/browser",
        "test": "tsc && qunit && jake test"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NovaSheets/NovaSheets.git"
    },
    "bugs": {
        "url": "https://github.com/NovaSheets/NovaSheets/issues"
    },
    "author": "Nixinova (https://nixinova.com)",
    "homepage": "https://novasheets.js.org",
    "license": "ISC",
    "dependencies": {
        "glob": "^7.1.7"
    },
    "devDependencies": {
        "@types/node": "ts4.3",
        "jake": "^10.8.2",
        "qunit": "^2.15.0",
        "typescript": "~4.3.2"
    }
}
